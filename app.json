{
  "expo": {
    "name": "Dota 2 Stats",
    "slug": "dota2",
    "version": "1.2.4",
    "orientation": "portrait",
    "icon": "./src/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": [
      "com.dota2stats.app"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.missinhoo.Dota2",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/images/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f378759e-7ae4-44ae-8bcf-d339513332f2"
      }
    },
    "owner": "emerson2342",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#002c3a",
          "image": "./src/images/splash.png",
          "imageWidth": 200
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4117477719356991~2847737808",
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      "expo-font",
      "expo-router"
    ]
  }
}